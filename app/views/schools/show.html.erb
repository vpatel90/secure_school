<h1>Schools#show</h1>


<div class="row">
  <%= link_to "Delete", school_path(@school.id), method: "DELETE",
              class: "delete-edit-button right red waves-effect waves-light btn",
              data: { confirm: "Are you sure you want to delete this School?" }%>

    <%= link_to "Edit", edit_school_path(@school.id),
                class: "delete-edit-button right orange waves-effect waves-light btn"%>

  <div class="col s12 m8 l10">
    <%= render partial: 'schoolbox', locals: { school: @school } %>
  </div>


</div>
